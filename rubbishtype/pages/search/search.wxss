view.search {
    display: flex;
    flex-direction: row;
    height: 98rpx;
    /* background: #efeff4; */
    align-items: center;
}

view.searchItem {
    display: flex;
    width: 100%;
    height: 30px;
    align-items: center;
    background: #efeff4;
    flex-direction: row;
    border-radius: 40rpx;
    margin-right: 20rpx;
    margin-left: 20rpx;
}
view.searchItem image {
    height: 42rpx;
    width: 42rpx;
    align-self: center;
    margin-left: 10rpx;
    margin-right: 20rpx;
}
view.searchItem input {
    flex: 1;
    font-size: 30rpx;
}
.serresult{
  line-height: 2.5;
  padding: 0 14px;
  font-size: 12px;
  color: seagreen;
}
.active{
  background: red;
}

.categ{
   color: salmon;
}
.categ1{
   color: seagreen;
}